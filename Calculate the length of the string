#include <stdio.h>

int main() {
    char s[100];
    printf("Enter a string: ");
    // read string with spaces (use fgets), or simpler: scanf without spaces:
    scanf("%s", s);
    int i = 0;
    while (s[i] != '\0') {
        i++;
    }
    printf("Length of given string = %d\n", i);
    return 0;
}
